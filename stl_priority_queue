#include <iostream>
#include <queue>
#include <algorithm>
using namespace std;
int main()
{
  cout << "asd" << endl;
  //test priority queue...
  int ia[9] = {0,1,2,3,4,8,9,3,5};
  priority_queue<int> ipq(ia,ia+9);
  cout << "size=" << ipq.size() << endl;
  while(!ipq.empty()){
    cout << ipq.top() << ' ' ;
    ipq.pop();
  }
  cout << endl;
  return 0;
}
